long long x, y, d;

void extendedEuclid(long A, long B) {
    if(B == 0){
        d = A;
        x = 1;
        y = 0;
    }
    else {
        extendedEuclid(B, A % B);
        int temp = x;
        x = y;
        y = temp - (A / B) * y;
    }
}
long long modInverse(long long A, long long M) {
    extendedEuclid(A ,M);
    return (x % M + M) % M;
}
